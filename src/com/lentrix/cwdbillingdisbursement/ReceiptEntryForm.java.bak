    private void customersFieldActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_customersFieldActionPerformed
        Customer selectedCustomer = (Customer)customersField.getSelectedItem();
        try {
            Object[] unsettledBills = selectedCustomer.getUnsettledBills().toArray();
            int rows = unsettledBills.length;
            String headers[] = {"ID","Period", "Date Due", "Balance"};
            Object contents[][] = new Object[rows][4];
            for(int i=0; i<rows; i++) {
                Billing billing = ((Billing)unsettledBills[i]);
                contents[i][0] = billing;
                contents[i][1] = billing.getMonthString() + " " + billing.getYear();
                contents[i][2] = billing.getDateDue();
                contents[i][3] = billing.getBalance();
            }
            
            DefaultTableModel model = new DefaultTableModel(contents,headers) {
                @Override
                public boolean isCellEditable(int row, int column) {
                    return false;
                }                
            };
            
            billsTable.setModel(model);
            billsTable.getColumnModel().getColumn(3).setCellRenderer(Base.getRightRender());
        }catch(SQLException ex) {
            Base.error(ex.getMessage(), this);
            ex.printStackTrace();
        }
    }//GEN-LAST:event_customersFieldActionPerformed

    private void billsTableMouseClicked(java.awt.event.MouseEvent evt) {//GEN-FIRST:event_billsTableMouseClicked
        try {
            Billing selectedBilling = (Billing)billsTable.getValueAt(billsTable.getSelectedRow(), 0);
            Balances balances = selectedBilling.getBalances();
            waterBillBalance.setText(String.format("%.2f",balances.getArrears()));
            meterSaleBalance.setText(String.format("%.2f", balances.getMeterSale()));
            NIBalance.setText(String.format("%.2f", balances.getNi()));
            PCABalance.setText(String.format("%.2f", balances.getPca()));
            relocBalance.setText(String.format("%.2f", balances.getReloc()));
            reconBalance.setText(String.format("%.2f", balances.getRecon()));
            others1Balance.setText(String.format("%.2f", balances.getOther1()));
            others2Balance.setText(String.format("%.2f", balances.getOther2()));
            others3Balance.setText(String.format("%.2f", balances.getOther3()));
        }catch(SQLException ex) {
            Base.error(ex.getMessage(), this);
            ex.printStackTrace();
        }
    }//GEN-LAST:event_billsTableMouseClicked

    
    
