    private void initCustomers() {
        try {
            LinkedList customerList = Customer.getAll(Base.getConnection(), "");
            ListIterator iter = customerList.listIterator();
            
            customersField.removeAllItems();
            while(iter.hasNext()) {
                Customer c = (Customer)iter.next();
                customersField.addItem(c);
            }
            
        }catch(SQLException ex) {
            Base.error(ex.getMessage(), this);
            ex.printStackTrace();
        }
    }
